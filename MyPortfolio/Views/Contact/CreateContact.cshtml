@model Contact
@{
    ViewData["Title"] = "CreateContact";
    Layout = "~/Views/Layout/Index.cshtml";
}

<div class="content">
    <div class="container-fluid">
        <h4 class="page-title">Yetenekler</h4>
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header">
                        <div class="card-title">Yetenek Ekleme Sayfası</div>
                    </div>
                    <div class="card-body">
                        <form method="post">

                            <label>Başlık</label>
                            <input type="text" asp-for="Title" class="form-control" />
                            <br />
                            <label>Açıklama</label>
                            <input type="text" asp-for="Description" class="form-control" />
                            <br />

                            <label>Tel 1</label>
                            <input type="text" asp-for="Phone1" class="form-control" />
                            <br />

                            <label>Tel 2</label>
                            <input type="text" asp-for="Phone2" class="form-control" />
                            <br />

                            <label>E-mail 1</label>
                            <input type="text" asp-for="Email1" class="form-control" />
                            <br />
                            

                            <label>E-mail 2</label>
                            <input type="text" asp-for="Email2" class="form-control" />
                            <br />

                            <label>Adres</label>
                            <input type="text" asp-for="Address" class="form-control" />
                            <br />




                            <button class="btn btn-primary">İletişim Bilgisini Ekle</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
